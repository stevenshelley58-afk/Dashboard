{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "description": "Agency dashboard monorepo",
  "workspaces": [
    "apps/web",
    "apps/worker",
    "packages/shared"
  ],
  "scripts": {
    "dev:web": "npm run dev --workspace web",
    "build:web": "npm run build --workspace web",
    "lint:web": "npm run lint --workspace web",
    "dev:worker": "npm run dev --workspace worker",
    "build:worker": "npm run build --workspace worker",
    "start:worker": "npm run start --workspace worker",
    "db:migrate": "npm run migrate --workspace worker"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevenshelley58-afk/Dashboard.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.18.0"
  },
  "packageManager": "npm@10.7.0"
}
